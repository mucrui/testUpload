#########################################################
###  Spring DataSource  -- DataSource configuration   ###
#########################################################
spring.datasource.url=jdbc:mysql://localhost:3306/user?useUnicode=true&characterEncoding=utf8&serverTimezone=GMT
spring.datasource.driverClassName=com.mysql.jdbc.Driver
spring.datasource.username=root
spring.datasource.password=123456
 
#mybatis映射文件配置
mybatis.mapper-locations=classpath:mapper/*.xml 

#项目名
server.servlet.context-path=/springbootDemo

#视图解析器   逻辑视图hello => /WEB-INF/jsp/ + hello + .jsp
spring.mvc.view.prefix=/WEB-INF/jsp/
spring.mvc.view.suffix=.jsp
